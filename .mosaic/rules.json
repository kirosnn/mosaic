{
  "bash": {
    "autoRun": [
      "mosaic mcp *",
      "git add *",
      "git commit *",
      "git push *",
      "git status *",
      "git diff *",
      "powershell *",
      "Get-ChildItem .git *",
      "Write-Output MERGE_HEAD: $(Test-Path .git\\MERGE_HEAD); *",
      "git rev-parse *"
    ]
  }
}